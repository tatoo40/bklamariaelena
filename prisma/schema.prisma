generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Usuario {
  id                      Int                   @id @default(autoincrement())
  createdAt               DateTime              @default(now())
  updatedAt               DateTime              @updatedAt
  id_mod                  Int                   @default(0)
  estado                  String                @default("S")
  email                   String                @unique
  hash                    String
  hashRt                  String?
  telefono_contacto       String?
  nombre                  String?
  apellido                String?
  isRegisteredWithGoogle  Boolean               @default(false)
  isAdmin                 Boolean               @default(false)
  token_recuperacion_pass String?
  id_rol                  Int
  id_idioma               Int?               
  roles                   Rol                   @relation(fields: [id_rol], references: [id])
  usuarios                usuarios_x_empresas[]
  permisos_x_usuario_seccion Permisos_x_usuario_seccion[]



  @@map("usuarios")
}

model Rol {
  id          Int       @id @default(autoincrement())
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  id_mod      Int       @default(0)
  estado      String    @default("S")
  descripcion String
  usuarios    Usuario[]

  permisos_x_rol_seccion Permisos_x_rol_seccion[] 

  @@map("roles")
}

model Moneda {
  id                 Int                  @id @default(autoincrement())
  createdAt          DateTime             @default(now())
  updatedAt          DateTime             @updatedAt
  id_mod             Int                  @default(0)
  estado             String               @default("S")
  descripcion        String
  simbolo            String
  cpt_fact_prov      Cpt_fact_prov[]
  tipo_cambio_diario Tipo_cambio_diario[]
  cpt_facturas       Cpt_facturas[]
  cpf_contaux       Cpf_contaux[]
    cpp_fact_prov        Cpp_fact_prov[]
    cpt_registros_gasto Cpt_registros_gasto[]
    cpp_facturas Cpp_facturas[]
    cpt_ingresoRomaneo Cpt_ingresoRomaneo[]
    cpp_ingresoRomaneo Cpp_ingresoRomaneo[]
    cpt_pago_fact_prov Cpt_pago_fact_prov[]

  @@map("monedas")
}

model Tipo_cambio_diario {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  id_mod    Int      @default(0)
  estado    String   @default("S")
  valor     Decimal
  fecha     DateTime
  id_moneda Int
  monedas   Moneda   @relation(fields: [id_moneda], references: [id])

  @@map("tipo_cambio_diario")
}

model Tipo_costeo {
  id          Int          @id @default(autoincrement())
  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt
  id_mod      Int          @default(0)
  estado      String       @default("S")
  descripcion String
  cpf_costos  Cpf_costos[]

  @@map("tipos_costeo")
}


model Idioma {
  id          Int          @id @default(autoincrement())
  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt
  id_mod      Int          @default(0)
  estado      String       @default("S")
  descripcion String
  prefijo String


  @@map("idiomas")
}


model Tipo_titular {
  id          Int       @id @default(autoincrement())
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  id_mod      Int       @default(0)
  estado      String    @default("S")
  descripcion String
  titulares   Titular[]

  @@map("tipos_titulares")
}

model Empresa {
  id                   Int                    @id @default(autoincrement())
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  id_mod               Int                    @default(0)
  estado               String                 @default("S")
  nombre               String
  razon_social         String?
  rut                  String
  direccion            String
  email_contacto       String
  telefono_contacto    String
  observaciones        String
  aprobada             Boolean?
  token                String?
  activa               Boolean?
  articulos            Articulo[]
  cpf_costos           Cpf_costos[]
  cpf_stockaux         Cpf_stockaux[]
  cpp_movimiento_stock Cpp_movimiento_stock[]
  empresas             usuarios_x_empresas[]
  cpt_registros_sanitarios Cpt_registro_sanitario[]
  cpp_registros_sanitarios Cpp_registro_sanitario[]
  cpf_regsitros_sanitario Cpf_registro_sanitario[]
  titulares Titular[]
  cpf_consumos Cpf_consumos[]
  cpt_identidad Cpt_identidad[]
  cpt_altaganado Cpt_altaganado[]
  cpt_parametros_x_empresa Cpt_parametros_x_empresa[]

  @@map("empresas")
}



model usuarios_x_empresas {
  id_usuario Int
  id_empresa Int
  createdAt  DateTime   @default(now())
  updatedAt  DateTime   @updatedAt
  estado               String                 @default("S")
  empresa    Empresa    @relation(fields: [id_empresa], references: [id])
  usuario    Usuario    @relation(fields: [id_usuario], references: [id])
  depositos  Deposito[]

  @@id([id_usuario, id_empresa])
}

model Deposito {
  id                   Int                 @id @default(autoincrement())
  createdAt            DateTime            @default(now())
  updatedAt            DateTime            @updatedAt
  id_mod               Int                 @default(0)
  estado               String              @default("S")
  nombre               String
  superficie           Int?
  dicose               String?
  latitud              String?
  longitud             String?
  superficie_arrendada Int?
  id_departamento      Int
  id_usuario           Int
  id_empresa           Int
  departamento         Departamento        @relation(fields: [id_departamento], references: [id])
  usuarios_x_empresas  usuarios_x_empresas @relation(fields: [id_usuario, id_empresa], references: [id_usuario, id_empresa])
  sector               Sector[]

  @@map("depositos")
}


model Sector {
  id                   Int                @id @default(autoincrement())
  createdAt            DateTime            @default(now())
  updatedAt            DateTime            @updatedAt
  id_mod               Int                 @default(0)
  estado               String              @default("S")
  nombre               String
  latitud              String?
  longitud             String?
  superficie           Int?
  id_deposito          Int
  id_empresa           Int
  deposito             Deposito    @relation(fields: [id_deposito],references: [id])
  embarcadero          Boolean?
  pasturas_x_sector       Pasturas_x_sector[] 
  cpf_consumos Cpf_consumos[]
  cpf_stockaux Cpf_stockaux[]
  
  @@map("sectores")


}

model Pasturas{
  id                   Int                @id @default(autoincrement())
  createdAt            DateTime            @default(now())
  updatedAt            DateTime            @updatedAt
  id_mod               Int                 @default(0)
  estado               String              @default("S")
  nombre               String
  pasturas_x_sector       Pasturas_x_sector[] 
  vida_util_dias            Int


  @@map("pasturas")
}


model Pasturas_x_sector{
  id                   Int                @id @default(autoincrement())
  createdAt            DateTime            @default(now())
  updatedAt            DateTime            @updatedAt
  id_mod               Int                 @default(0)
  estado               String              @default("S")
  descripcion               String
  observaciones               String
  fecha_siembra     DateTime
  fecha_pastoreo    DateTime
  fecha_estimada_fin_pastoreo DateTime
  fecha_fin_pastoreo DateTime
  id_pastura Int
  id_sector  Int
  id_empresa           Int  
  pasturas             Pasturas    @relation(fields: [id_pastura],references: [id])
  sectores             Sector    @relation(fields: [id_sector],references: [id])


  @@map("pasturas_x_sector")
}


model Cpt_recaravaneo{
  id                   Int                @id @default(autoincrement())
  createdAt            DateTime            @default(now())
  updatedAt            DateTime            @updatedAt
  id_mod               Int                 @default(0)
  estado               String              @default("S")
  observaciones               String
  fecha    DateTime
  cod_articulo    String
  id_categoria_ganado Int
  id_marca_ganado Int
  nro_lote String
  cod_identidad String
  cod_identidad_nuevo String
  id_empresa Int
  id_sexo Int
  articulos             Articulo    @relation(fields: [cod_articulo],references: [cod_articulo])



  @@map("cpt_recaravaneo")
}


model Cpt_evolucion_ganado{
  id                   Int                @id @default(autoincrement())
  createdAt            DateTime            @default(now())
  updatedAt            DateTime            @updatedAt
  id_mod               Int                 @default(0)
  estado               String              @default("S")
  cod_articulo    String
  cod_articulo_evoluciona    String
  peso_fin Decimal
  articulos             Articulo    @relation("Articulos",fields: [cod_articulo],references: [cod_articulo])
  articulos_evoluciona  Articulo    @relation("ArticulosEvoluciona",fields: [cod_articulo_evoluciona],references: [cod_articulo])

  @@map("cpt_evolucion_ganado")
}


model Cpt_registro_lluvias{
  id                   Int                @id @default(autoincrement())
  createdAt            DateTime            @default(now())
  updatedAt            DateTime            @updatedAt
  id_mod               Int                 @default(0)
  estado               String              @default("S")
  cantidad_mm    Decimal
  fecha    DateTime
  observacion String
  id_empresa Int

  @@map("cpt_registro_lluvias")
}


model Departamento {
  id          Int        @id @default(autoincrement())
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  estado      String     @default("S")
  descripcion String
  depositos   Deposito[]

  @@map("departamentos")
}

model Estado_ganado {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  estado      String   @default("S")
  descripcion String

  @@map("estados_ganado")
}

model Propiedad_ganado {
  id                   Int                    @id @default(autoincrement())
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  estado               String                 @default("S")
  descripcion          String
  cpt_movimiento_stock Cpt_movimiento_stock[]

  @@map("propiedad_ganado")
}

model Tipo_ganado {
  id                Int                @id @default(autoincrement())
  createdAt         DateTime           @default(now())
  updatedAt         DateTime           @updatedAt
  estado            String             @default("S")
  descripcion       String
  categorias_ganado Categoria_ganado[]
  marcas_ganado     Marca_ganado[]

  @@map("tipos_ganado")
}

model Categoria_ganado {
  id             Int         @id @default(autoincrement())
  createdAt      DateTime    @default(now())
  updatedAt      DateTime    @updatedAt
  estado         String      @default("S")
  descripcion    String
  id_tipo_ganado Int
  rango_dia_inicio Int?
  rango_dia_fin  Int?
  cantidad_dientes Int?
  abreviacion      String?
  sexo           String?
  predeterminada Boolean?
  tipo_ganado    Tipo_ganado @relation(fields: [id_tipo_ganado], references: [id])
  articulos      Articulo[]
  cpt_identidad Cpt_identidad[]
  cpt_altaganado Cpt_altaganado[]
  @@map("categorias_ganado")
}

model Titular {
  id                   Int             @id @default(autoincrement())
  createdAt            DateTime        @default(now())
  updatedAt            DateTime        @updatedAt
  id_mod               Int             @default(0)
  estado               String          @default("S")
  nombre_fantasia      String
  razon_social         String?
  rut                  String
  direccion            String
  email                String
  observaciones        String
  nombre_contacto      String
  telefono_contacto    String
  id_tipo_titular      Int
  id_categoria_prov    Int
  id_categoria_cliente Int
  id_empresa           Int?
  categoria_cli        Catelgoria_cli  @relation(fields: [id_categoria_cliente], references: [id])
  categoria_prov       Categoria_prov  @relation(fields: [id_categoria_prov], references: [id])
  tipo_titular         Tipo_titular    @relation(fields: [id_tipo_titular], references: [id])
  empresas         Empresa?    @relation(fields: [id_empresa], references: [id])
  cpt_fact_prov        Cpt_fact_prov[]
  cpp_fact_prov        Cpp_fact_prov[]
  cpt_facturas       Cpt_facturas[]
  cpf_contaux       Cpf_contaux[]
   articulos_x_titular Articulos_x_titular[]
   cpt_registros_gasto Cpt_registros_gasto[]
   cpp_facturas Cpp_facturas[]
   cpt_ingresoRomaneo Cpt_ingresoRomaneo[]
   cpp_ingresoRomaneo Cpp_ingresoRomaneo[]
   cpt_pago_fact_prov Cpt_pago_fact_prov[]


  @@map("titulares")
}

model Catelgoria_cli {
  id          Int       @id @default(autoincrement())
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  id_mod      Int       @default(0)
  estado      String    @default("S")
  descripcion String
  titulares   Titular[]

  @@map("categorias_cliente")
}

model Categoria_prov {
  id          Int       @id @default(autoincrement())
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  id_mod      Int       @default(0)
  estado      String    @default("S")
  descripcion String
  titulares   Titular[]

  @@map("categorias_prov")
}

model Marca_ganado {
  id             Int         @id @default(autoincrement())
  createdAt      DateTime    @default(now())
  updatedAt      DateTime    @updatedAt
  estado         String      @default("S")
  descripcion    String
  cod_snig       String?
  id_tipo_ganado Int
  abreviacion      String?
  tipo_ganado    Tipo_ganado @relation(fields: [id_tipo_ganado], references: [id])
  articulos      Articulo[]
  cpt_identidad  Cpt_identidad[]
  cpt_altaganado Cpt_altaganado[]
  
  @@map("marcas_ganado")

}

model Estado_stock {
  id           Int            @id @default(autoincrement())
  createdAt    DateTime       @default(now())
  updatedAt    DateTime       @updatedAt
  estado       String         @default("S")
  descripcion  String
  es_fisico    String         @default("S")
  cpf_stockaux Cpf_stockaux[]
  cpf_consumos Cpf_consumos[]
  @@map("estados_stock")
}

model Tipo_toma_peso {
  id                   Int                    @id @default(autoincrement())
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  estado               String                 @default("S")
  descripcion          String
  cpt_movimiento_stock Cpt_movimiento_stock[]

  @@map("tipos_toma_peso")
}


model Unidades {
  id                Int            @id @default(autoincrement())
  createdAt         DateTime       @default(now())
  updatedAt         DateTime       @updatedAt
  estado            String         @default("S")
  descripcion       String
  descripcion_corta String
  articulos_cmp         Articulo[] @relation("unidad_cmp")
  articulos_vta         Articulo[] @relation("unidad_vta")
  articulos_stk         Articulo[] @relation("unidad_stk")
  cpf_costos        Cpf_costos[]
  cpf_stockaux      Cpf_stockaux[]
  cpf_consumos Cpf_consumos[]
  @@map("unidades")
}

model Motivo_sanitario {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  estado      String   @default("S")
  descripcion String
  frecuencia Int?
  unidad_frecuencia String?
  recurrente Boolean?
  dosis Decimal?
  id_empresa           Int?
  cod_articulo String?
  articulos    Articulo?  @relation(fields: [cod_articulo], references: [cod_articulo])


  cpt_registros_sanitarios Cpt_registro_sanitario[]
  cpp_regsitros_sanitario Cpp_registro_sanitario[]  
  cpf_regsitros_sanitario Cpf_registro_sanitario[] 

  @@map("motivos_sanitarios")
}

model Tipos_mov_stock {
  id                Int                @id @default(autoincrement())
  createdAt         DateTime           @default(now())
  updatedAt         DateTime           @updatedAt
  estado            String             @default("S")
  descripcion       String
  naturaleza        Int
  motivos_mov_stock Motivo_mov_stock[]


  @@map("tipos_mov_stock")
}

model Motivo_mov_stock {
  id                   Int                    @id @default(autoincrement())
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  estado               String                 @default("S")
  descripcion          String
  cod_docum            String
  id_tipo_mov_stk      Int
  tipo_mov_stk         Tipos_mov_stock        @relation(fields: [id_tipo_mov_stk], references: [id])
  cpf_stockaux         Cpf_stockaux[]
  cpf_consumos         Cpf_consumos[]
  cpt_movimiento_stock Cpt_movimiento_stock[]

  @@map("motivos_mov_stock")
}

model Articulo {
  id                   Int                    @id @default(autoincrement())
  cod_articulo         String                 @unique
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  estado               String                 @default("S")
  nombre               String
  id_unidad_stk        Int
  id_unidad_vta        Int?
  id_unidad_cmp        Int?
  id_categoria_ganado  Int
  id_marca_ganado      Int
  id_empresa           Int
  id_tipo_articulo     Int?
  id_tasa_iva_cmp      Int?
  id_tasa_iva_vta      Int?
  factor_conv_cmp_a_stk Decimal?
  categorias_ganado    Categoria_ganado       @relation(fields: [id_categoria_ganado], references: [id])
  empresas             Empresa                @relation(fields: [id_empresa], references: [id])
  marcas_ganado        Marca_ganado           @relation(fields: [id_marca_ganado], references: [id])
  unidad_stk           Unidades?           @relation("unidad_stk",fields: [id_unidad_stk], references: [id])
  unidad_vta           Unidades?           @relation("unidad_vta",fields: [id_unidad_vta], references: [id])
  unidad_cmp           Unidades?           @relation("unidad_cmp",fields: [id_unidad_cmp], references: [id])
  tasas_iva_cmp            Tasas_iva?           @relation("tasa_iva_cmp",fields: [id_tasa_iva_cmp], references: [id])
  tasas_iva_vta            Tasas_iva?          @relation("tasa_iva_vta",fields: [id_tasa_iva_vta], references: [id])
  tipo_art             Tipo_articulo?         @relation(fields: [id_tipo_articulo], references: [id])


  cpf_costos           Cpf_costos[]
  cpf_stockaux         Cpf_stockaux[]
  cpp_movimiento_stock Cpp_movimiento_stock[]
  cpt_movimiento_stock Cpt_movimiento_stock[]
  cpp_registros_sanitarios Cpp_registro_sanitario[]
  cpp_fact_prov        Cpp_fact_prov[]
  articulos_x_titular Articulos_x_titular[]
  cpt_recaravaneo     Cpt_recaravaneo[]
  motivos_sanitario    Motivo_sanitario[]
  cpt_registro_sanitario Cpt_registro_sanitario[]
  cpt_evolucion_ganado      Cpt_evolucion_ganado[] @relation("Articulos")
  cpt_evolucion_ganado_nuevo Cpt_evolucion_ganado[] @relation("ArticulosEvoluciona")
  cpf_consumos Cpf_consumos[]
  cpp_facturas Cpp_facturas[]
  cpp_ingresoRomaneo Cpp_ingresoRomaneo[]
  cpf_regsitros_sanitario Cpf_registro_sanitario[] 
  cpt_identidad Cpt_identidad[] 
  cpt_altaganado Cpt_altaganado[]


  @@map("articulos")
}

model Tipo_articulo{
  id                   Int                    @id @default(autoincrement())
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  estado               String                 @default("S")
  descripcion               String

  articulos    Articulo[]

  @@map("tipo_articulo")

}


model Tipos_gasto{
  id                   Int                    @id @default(autoincrement())
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  estado               String                 @default("S")
  descripcion               String

  cpt_registros_gasto    Cpt_registros_gasto[]

  @@map("tipos_gasto")

}

model Tasas_iva{
  id                   Int                    @id @default(autoincrement())
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  estado               String                 @default("S")
  descripcion               String
  porcentaje Decimal

  @@map("tasas_iva")

  articulos_cmp Articulo[] @relation("tasa_iva_vta")
  articulos_vta Articulo[] @relation("tasa_iva_cmp")
  cpp_fact_prov        Cpp_fact_prov[]

}

model Cpt_registros_gasto{
  id                   Int                    @id @default(autoincrement())
  nro_trans       Int
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  estado               String                 @default("S")
  descripcion               String
  fecha DateTime
  cod_docum String
  importe_mo    Decimal?     
  importe_mn    Decimal?
  importe_tr    Decimal?
  tc            Decimal
  id_moneda       Int
  id_titular      Int
  id_empresa      Int       @default(1)
  monedas         Moneda    @relation(fields: [id_moneda], references: [id])
  titulares       Titular   @relation(fields: [id_titular], references: [id])
  id_tipo_gasto Int
  tipos_gasto Tipos_gasto @relation(fields: [id_tipo_gasto], references: [id])


  @@map("cpt_registros_gasto")

}


model Articulos_x_titular{

  id                   Int                    @id @default(autoincrement())
  cod_articulo         String                 
  createdAt            DateTime               @default(now())
  updatedAt            DateTime               @updatedAt
  estado               String                 @default("S")
  id_titular             Int
  id_empresa           Int?

  titulares Titular @relation(fields: [id_titular], references: [id])
  articulos Articulo @relation(fields: [cod_articulo], references: [cod_articulo])

  @@map("articulos_x_titular")
}

model Cpf_consumos {
  id              Int              @id @default(autoincrement())
  nro_trans       Int
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt
  estado          String           @default("S")
  cantidad        Decimal?         @default(1)
  cantidad2       Decimal?
  cantidad3       Decimal?
  signo           Int?
  fecha           DateTime?
  id_motivo_stk   Int
  cod_articulo    String
  id_unidad_stk   Int
  id_empresa      Int
  id_sector     Int
  id_estado_stock Int
  articulos       Articulo         @relation(fields: [cod_articulo], references: [cod_articulo])
  sectores        Sector         @relation(fields: [id_sector], references: [id])
  empresas        Empresa          @relation(fields: [id_empresa], references: [id])
  estados_stock   Estado_stock     @relation(fields: [id_estado_stock], references: [id])
  motivos_stock   Motivo_mov_stock @relation(fields: [id_motivo_stk], references: [id])
  unidad_stk      Unidades     @relation(fields: [id_unidad_stk], references: [id])



  @@map("cpf_consumos")
}



model Cpf_stockaux {
  id              Int              @id @default(autoincrement())
  nro_trans       Int
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt
  estado          String           @default("S")
  cantidad        Decimal?         @default(1)
  cantidad2       Decimal?
  cantidad3       Decimal?
  signo           Int?
  nro_lote        String?     
  cod_identidad   String?
  fecha           DateTime?
  id_motivo_stk   Int
  cod_articulo    String
  id_unidad_stk   Int
  id_empresa      Int
  id_sector     Int
  id_estado_stock Int
  articulos       Articulo         @relation(fields: [cod_articulo], references: [cod_articulo])
